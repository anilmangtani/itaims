
    <head>
        <title>My Site</title>
        
        <link rel="stylesheet" href="/static/css/care.css">
      </head>  
        
        <body> 
          <main>
            <div class="heading-gift-container">
              <div class="gift-page-heading">
                <h1>Fruits</h1>
              </div>
              <div class="gift-page-desc">
                <p>Plants are like Child, Care Them like a child </p>
              </div>
            </div>
           <div class="pageItemFlex">
            <% productitems.forEach(function(productitems) { %>
                  <section>
              
                  <div class="pgitems-heading"></div>
                  <div class="pgitems-products">
                    <div class="pgitem-prdct1 sim-bs-itemspg">
                      <div class="pgitem-prdct-image"><a href=""><img src="<%= productitems.img %>" alt="" srcset="" width="100%" height="100%"/></a></div>
                      <div class="pgitem-item-info-container">   
                        <div class="pgitem-prdt-info">
                          <h3><%= productitems.name %></h3>
                          <p>Rs<%= productitems.price %></p>
                        </div>
                        <div class="pgitem-prdt-link">
                          
                          <button data-productitems ="<%= JSON.stringify(productitems) %>" class="AtcBtn CartButtonC"  id="CartButtonI">+Add to cart</button>
                          <button data-productitems ="<%= JSON.stringify(productitems) %>" class="RmvBtn CartButtonC"  id="CartButtonI">Remove from cart</button>
                        </div>
                      </div>
                    </div>
                  </div>
                </section>
  
            <% }) %>
           </div>
          </main>
          <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
          <script   src="/static/js/cart.js"></script>
        </body>
      </head>
    </html>